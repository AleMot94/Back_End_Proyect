<div>
    <h1>productos</h1>
    <div id="div-products">
       
    </div>

    
    <h2>agregar un producto</h2>
    
    <form>
        <label>Nombre</label>
        <input type="text" id="title"/>
        <label>Descripcion</label>
        <textarea name="description" id="description" cols="30" rows="10" type="text" ></textarea>
        <label>Precio</label>
        <input id="price" type="number"/>
        <label>Imagen</label>
        <input type="file" id="thumbnail"/>
        <label>Codigo</label>
        <input type="text"  id="code"/>
        <label>Stock</label>
        <input type="number" id="stock"/>
        <label>Status</label>
        <input type="text" id="status"/>
        <input type="submit"/>
    </form>

</div>
<script src="/socket.io/socket.io.js"></script>
<script src="/js/products-socket.js"></script>